{
    "name": "xsolve-pl/xsolve-google-auth-bundle",
    "description": "XSolve Google Auth Bundle to provide signing in to FOSUserBundle with Google Auth",
    "license": "MIT",
    "autoload": {
        "psr-0": {
            "Xsolve\\GoogleAuthBundle": ""
        }
    },
    "require": {
        "friendsofsymfony/user-bundle": "v1.3.3",
        "google/google-api-php-client": "0.6.*"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "google/google-api-php-client",
                "version": "0.6.0",
                "dist": {
                    "url": "http://google-api-php-client.googlecode.com/files/google-api-php-client-0.6.0.tar.gz",
                    "type": "tar"
                },
                "autoload": {
                    "classmap": ["src/"]
                }
            }
        }

    ],
    "target-dir" : "Xsolve/GoogleAuthBundle"
}
